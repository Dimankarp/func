int strlen(string s){
    int len = 0;
    while(s[len] != 0)
        len = len + 1;
    len = len + 1;
    return len;
}

void print_str(string s, int len){
    int i = 0;
    while(i < len){
        print(s[i]);
        i = i + 1;
    }
    return;
}

int main(){
    string a = "i love Paris  in the moring";
    string b = "Moscow";
    int i = 7;
    while(i < strlen("Moscow") - 1 + 7){
        a[i] = b[i-7];
        i = i + 1;
    }
    print_str(a, strlen(a));
    return;
}
