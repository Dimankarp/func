int write(int _);
int read();
void exit(int _);


int strlen(string s);
void write_str(string s, int len);
int itoascii(int n);
void write_int(int n);
bool is_digit(int n);
int read_int();


int factorial(int n) {
  if (n == 1 || n == 0)
    return 1;
  return n * factorial(n - 1);
}

void main() {
  string msg = "Please, enter number:\n";
  write_str(msg, strlen(msg) - 1);
  int res = factorial(read_int());
  write_int(res);
  write(10);
  exit(0);
}
